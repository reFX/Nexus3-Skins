{
	// Palette of your own colors that can then be used for the skin
	"palette": {

		"--main": "#ccc",
		"--medium": "#888",
		"--dark": "#333"
	},

	// Front panel properties
	"panel": {

		// Here we define all colors for the panel
		"colors": {

			// General
			"background": "--main",
			"label": "--dark",
			"shadows": "#0008",
			"noise": "#0000000c",

			// Groups
			"groups": {

				"background": "--dark",
				"label": "--main",

				"scrollbars": {
	
					"background": "#000",
					"thumb": "#888",
					"thumb-hover": "#ccc"
				}
			},

			// Dials
			"dials": {

				"ring": "#969799",

				"outline": "#0002",
				"top": "#d7d7d5",
				"bottom": "#c2bdb9",

				"bevel": {

					"highlight": "#ebebe9",
					"shadow": "#0004"
				},

				"line": "#3b3c3e"
			},

			"effects": {

				"modulated": "deeppink"
			},

			"scrollbars": {
	
				"background": "#0000",
				"thumb": "#444",
				"thumb-hover": "#fff"
			}
		},

		// Here we define panel-dials properties
		"dial-style": {

			"outline": 2,
			"top-bevel": 1.5,
			"ring-width": 2,
			"ring-space": 5,
			"shadow-size": 0.6,
			"shadow-distance-y": 0.3,
			"pointer-space": 3,
			"pointer-width": 2.5,
			"light-angle-min": -5,
			"light-angle-max": 5
		},

		// Here we define note-event properties
		"events": {
			"note-depth": 0.2,
			"note-radius": 2,
			"note-outline": 0.5
		}
	},

	// Header properties
	"header": {

		// Here we define all colors for the header
		"colors": {

			// General
			"background": "--main",
			"label": "--dark",
			"avatar-outline": "--medium"
		},

		// Here we define header-dials properties
		"dial-style": {

			"outline": 1.5
		}
	},

	// Preview-dial
	"preview": {

		// Here we define preview-dial properties
		"dial-style": {

			"outline": 1,
			"ring-space": 2,	// only for preview-dial: limited to 0-2
			"ring-width": 0,	// only for preview-dial: limited to 0-2
			"shadow-size": 0.33,
			"shadow-distance-y": 0.3
		}
	},

	// Here we define the virtual keyboard
	"keyboard": {

		"realistic": true,
		"reflections": 0.2
	}
}
